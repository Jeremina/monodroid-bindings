<enum-method-mappings>
  <mapping jni-class="com/google/android/maps/MapController">
		<method jni-name="onKey"              parameter="keyCode"     clr-enum-type="Android.Views.Keycode" />
	</mapping>
  <mapping jni-class="com/google/android/maps/MapActivity">
		<method jni-name="onGetMapDataSource" parameter="return"      clr-enum-type="Android.GoogleMaps.MapDataSource" />
	</mapping>
  <mapping jni-class="com/google/android/maps/OverlayItem">
		<method jni-name="getMarker"          parameter="stateBitset" clr-enum-type="Android.GoogleMaps.ItemState" />
	</mapping>
  <mapping jni-class="com/google/android/maps/MyLocationOverlay">
		<method jni-name="onStatusChanged"    parameter="status"      clr-enum-type="Android.Locations.Availability" />
  	<method jni-name="onAccuracyChanged"  parameter="sensor"      clr-enum-type="Android.Hardware.SensorType" />
  	<method jni-name="onAccuracyChanged"  parameter="accuracy"    clr-enum-type="Android.Hardware.SensorStatus" />
  	<method jni-name="onSensorChanged"    parameter="sensor"      clr-enum-type="Android.Hardware.SensorType" />
	</mapping>
</enum-method-mappings>
